# Terraform variables file
# Copy this to terraform.tfvars and fill in your values

# AWS Configuration
aws_region = "us-east-1"

# EC2 Key Pair (create this in AWS Console first)
key_name = "ornakala-keypair"

# Your domain name
domain_name = "ornakala.com"

# CIDR block allowed to SSH (change to your IP for better security)
# Get your IP from: curl -s https://ifconfig.me
allowed_ssh_cidr = "0.0.0.0/0"